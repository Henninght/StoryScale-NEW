{
  "name": "storyscale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverage",
    "test:unit": "jest --testPathPattern='\\.(test|spec)\\.tsx?$' --testPathIgnorePatterns='e2e|integration'",
    "test:integration": "jest --testPathPattern='integration.*\\.(test|spec)\\.tsx?$'",
    "test:api": "jest --testPathPattern='api.*\\.(test|spec)\\.tsx?$'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:clear": "jest --clearCache",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:ci": "playwright test --reporter=github",
    "test:all": "npm run test:unit && npm run test:api && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.54.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "ioredis": "^5.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "14.2.31",
    "openai": "^5.12.1",
    "react": "^18",
    "react-dom": "^18",
    "redis": "^5.8.0",
    "stripe": "^18.4.0",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.6",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-jest": "^29.7.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.31",
    "identity-obj-proxy": "^3.0.0",
    "jest-extended": "^4.0.2",
    "jest-watch-typeahead": "^2.2.2",
    "node-fetch": "^3.3.2",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
